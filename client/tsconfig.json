  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "extends": "expo/tsconfig.base"
}
